<div class="row">
  <div class="col-md-12">
    <ol class="breadcrumb">
      <%= list_item_to_members %>
      <%= list_item_to_member(@member) %>
      <%= list_item_to_edit %>
    </ol>
  </div>
</div>
<div class="row">
  <div class="col-md-12">
    <ul class="nav nav-tabs">
      <%= tab_to_member(@member, active: true) %>
      <%= tab_to_members_courses(@member) %>
      <%= tab_to_members_rolls(@member) %>
      <%= tab_to_members_recesses(@member) %>
      <%= tab_to_members_receipts(@member) %>
    </ul>
  </div>
</div>
<br />
<div class="row">
  <div class="col-md-12">
  <%= render "form" %>
  <%= button_to_save pull: "right" %>
  <%= button_to_back members_path, pull: "left" %>
  </div>
</div>
