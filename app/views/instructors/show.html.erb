<% breadcrumb :instructor, @instructor %>
<div class="row">
  <div class="col-md-12">
    <ul class="nav nav-tabs">
      <%= tab_to_instructor @instructor, active: true %>
      <%= tab_to_instructors_courses @instructor %>
    </ul>
  </div>
</div>
<br />
<div class="row">
  <div class="col-md-12">
    <%= render "shared/notice" %>
    <%= render "form" %>
    <%= button_to_back instructors_path, pull: "left" %>
    <%= button_to_edit edit_instructor_path(@instructor), pull: "right" %>
  </div>
</div>
