<% breadcrumb :bank_account, @bank_account %>
<%= render partial: 'nav_tabs', locals: { bank_account: @bank_account, active: :bank_account } %>
<br />
<div class="row">
  <div class="col-md-12">
    <%= render "shared/notice" %>
    <%= render "form" %>
    <%= button_to_back bank_accounts_path, pull: "left" %>
    <% if @member.present? %>
      <%= button_to_edit edit_bank_account_path(@bank_account, member_id: @member.id), pull: "right" %>
    <% else %>
      <%= button_to_edit edit_bank_account_path(@bank_account), pull: "right" %>
    <% end %>
  </div>
</div>
